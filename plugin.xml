<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-yoco" version="1.0.0">
    <name>Yoco</name>
    <js-module name="Yoco" src="www/Yoco.js">
        <clobbers target="Yoco" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="Yoco">
                <param name="ios-package" value="Yoco" />
            </feature>
        </config-file>
        <source-file src="src/ios/Yoco.m" />
    </platform>
</plugin>
